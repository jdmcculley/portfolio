                    {
                      "type": "text",
                      "id": "kkoYi",
                      "name": "toolP1",
                      "fill": "$text-soft",
                      "textGrowth": "fixed-width",
                      "width": "fill_container",
                      "content": "When a functional app can emerge from a prompt in ten minutes, the value of design doesn't disappear. It relocates. The tool does the generating. You do the recognizing.",
                      "lineHeight": 1.7,
                      "fontFamily": "Inter",
                      "fontSize": 16,
                      "fontWeight": "normal"
                    },
