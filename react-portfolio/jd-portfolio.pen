                  "name": "Manifesto Text",
                  "fill": "$text",
                  "textGrowth": "fixed-width",
                  "width": "fill_container",
                  "content": "Craft without speed is a luxury.\nSpeed without craft is a liability.",
                  "lineHeight": 1.6,
                  "textAlign": "center",
                  "fontFamily": "Space Grotesk",
